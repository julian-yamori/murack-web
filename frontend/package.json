{
  "name": "murack-web-prototype-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "gen-api": "mkdir -p temp && curl -fsS http://0.0.0.0:3000/api/docs/openapi.json > temp/backend_api.json && orval --config orval.config.cjs",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swr": "^2.3.4",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.3.0",
    "orval": "^7.10.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5"
  }
}
